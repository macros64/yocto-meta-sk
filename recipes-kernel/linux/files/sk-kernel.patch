+		PVDD-supply = <&reg_3p3v>;
+		csi_id = <0>;
+		mclk = <24000000>;
+		mclk_source = <0>;
+		cvbs = <1>;
+		cam_std = <0>; /* 0 - PAL(default), 1 - NTSC J , 2 - NTSC M */
