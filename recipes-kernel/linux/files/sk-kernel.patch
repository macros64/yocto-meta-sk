+static int to_tw6869_pixformat(unsigned int pixelformat)
+{
+	int ret;
+
+	switch (pixelformat) {
+	case V4L2_PIX_FMT_YUYV:
+		ret = TW_FMT_YUYV;
+		break;
+	case V4L2_PIX_FMT_UYVY:
+		ret = TW_FMT_UYVY;
+		break;
+	case V4L2_PIX_FMT_RGB565:
+		ret = TW_FMT_RGB565;
+		break;
+	default:
+		ret = -EINVAL;
+	}
+	return ret;
+}
